    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naoray\\BlueprintNovaAddon\\BlueprintNovaAddonServiceProvider"
            ],
            "aliases": {
                "BlueprintNovaAddon": "Naoray\\BlueprintNovaAddon\\BlueprintNovaAddonFacade"
